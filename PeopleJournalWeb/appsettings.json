{
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 5,
      "MaxConcurrentUpgradedConnections": 5
    },
    "DisableStringResue" : true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",

    },
    "Debug": {
      "LogLevel": {
        "Default": "Debug"

      }
    }
  },
  "AllowedHosts": "*"
}
